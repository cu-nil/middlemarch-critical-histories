@book{RadwayReadingRomanceWomen1991,
  title = {Reading the {{Romance}}: {{Women}}, {{Patriarchy}}, and {{Popular Literature}}},
  shorttitle = {Reading the {{Romance}}},
  author = {Radway, Janice A.},
